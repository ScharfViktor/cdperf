{
	"root": true,
	"extends": ["@ownclouders"],
  "ignorePatterns": ["!*.test.ts"],
  "overrides": [
    {
      "files": ["src/**/*.ts"],
      "rules": {
        "@typescript-eslint/naming-convention": "off",
        // k6 uses the default export function as vu stage
        "import/no-default-export": "off"
      }
    }
  ]
}
